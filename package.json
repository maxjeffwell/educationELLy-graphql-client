{
  "name": "educationelly-graphql-client",
  "version": "1.0.0",
  "author": "Jeff Maxwell <maxjeffwell@gmail.com>",
  "license": "GNU GPLv3",
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "axios": "^1.7.7",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.10.1",
    "history": "^4.10.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.5",
    "serve": "^14.2.3",
    "styled-components": "^5.3.11",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,css,md}",
    "type-check": "echo 'TypeScript not configured yet'",
    "quality": "npm run lint && npm run format:check && npm run type-check",
    "prepare": "husky",
    "heroku-postbuild": "npm run build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2"
  },
  "overrides": {
    "nth-check": "^2.0.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^4.15.2"
  }
}
